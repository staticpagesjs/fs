{
  "name": "@static-pages/io",
  "version": "0.0.8",
  "description": "Helpers for reading and writing data.",
  "type": "module",
  "module": "./index.js",
  "main": "./index.cjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./node": {
      "import": "./node/index.js",
      "require": "./node/index.js",
      "types": "./node/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "deno": ">=1.0.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run test",
    "postversion": "git push && git push --tags",
    "clean": "node scripts/clean.js",
    "build": "node scripts/build.js",
    "watch": "node scripts/watch.js",
    "test": "mocha",
    "coverage": "c8 -r text -r text-summary -r lcov --include \"*.js\" npm test",
    "lint": "eslint src"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "js-yaml": "^3.14.1",
    "picomatch": "^3.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "@types/picomatch": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "c8": "^9.1.0",
    "eslint": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "author": "László BULIK",
  "license": "MPL-2.0",
  "homepage": "https://staticpagesjs.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/io.git"
  },
  "bugs": {
    "url": "https://github.com/staticpagesjs/io/issues"
  },
  "keywords": [
    "static",
    "pages",
    "generator",
    "renderer",
    "mvc",
    "jamstack",
    "view",
    "template",
    "controller"
  ]
}
