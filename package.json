{
  "private": true,
  "name": "@static-pages/fs",
  "version": "1.1.0",
  "description": "Provides helpers for the static pages generator to read from and write to an arbitary filesystem.",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.mts"
    },
    "./node": {
      "require": "./node.cjs",
      "import": "./node.mjs",
      "types": "./node.d.mts"
    },
    "./node.cjs": {
      "require": "./node.cjs",
      "types": "./node.d.cts"
    },
    "./node.mjs": {
      "import": "./node.mjs",
      "types": "./node.d.mts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16.0.0",
    "deno": ">=1.0.0"
  },
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "node scripts/build.mjs",
    "watch": "node scripts/watch.mjs",
    "test": "mocha",
    "coverage": "c8 -r text -r text-summary -r lcov --include \"**/*.mjs\" npm test",
    "prepublishOnly": "echo Default 'npm publish' disabled, use 'npm run publish' instead && exit 1",
    "publish": "npm run build && npm run test && cd package && npm publish",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "js-yaml": "^3.14.1",
    "picomatch": "^4.0.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/picomatch": "^2.3.3",
    "c8": "^9.1.0",
    "chokidar": "^3.6.0",
    "mocha": "^10.4.0",
    "semver": "^7.6.2",
    "typescript": "^5.4.5"
  },
  "author": "László BULIK",
  "license": "MPL-2.0",
  "homepage": "https://staticpagesjs.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/io.git"
  },
  "bugs": {
    "url": "https://github.com/staticpagesjs/io/issues"
  },
  "keywords": [
    "static",
    "pages",
    "generator",
    "renderer",
    "mvc",
    "jamstack",
    "view",
    "template",
    "controller"
  ]
}
